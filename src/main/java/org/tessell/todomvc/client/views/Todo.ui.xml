<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:gwt="urn:import:com.google.gwt.user.client.ui">
  <ui:with field="bs" type="org.tessell.todomvc.client.resources.BaseStyle" />
  <gwt:HTMLPanel ui:field="li" tag="li">
    <div class="{bs.view}">
      <gwt:SimpleCheckBox ui:field="checkBox" styleName="{bs.toggle}" />
      <gwt:Label ui:field="label" styleName="{bs.label}" />
      <gwt:Button ui:field="destroy" styleName="{bs.destroy}" />
    </div>
    <gwt:TextBox ui:field="editBox" styleName="{bs.edit}" />
  </gwt:HTMLPanel>
</ui:UiBinder>