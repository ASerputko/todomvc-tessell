<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:gwt="urn:import:com.google.gwt.user.client.ui">
  <ui:with field="bs" type="org.tessell.todomvc.client.resources.BaseStyle" />
  <gwt:HTMLPanel>
    <span class="{bs.todoCount}">
      <span ui:field="numberLeft" style="font-weight: bold" />
      <span ui:field="numberLeftWord" />
      left.
    </span>
    <gwt:Button ui:field="clearCompleted" styleName="{bs.clearCompleted}"/>
  </gwt:HTMLPanel>
</ui:UiBinder>