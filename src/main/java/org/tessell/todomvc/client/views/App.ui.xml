<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:gwt="urn:import:com.google.gwt.user.client.ui">
  <ui:with field="bs" type="org.tessell.todomvc.client.resources.BaseStyle" />
  <gwt:HTMLPanel ui:field="root">
    <section class="{bs.todoapp}">
      <header class="{bs.header}">
        <div ui:field="headerPlaceholder" />
      </header>
      <section ui:field="main" class="{bs.main}">
        <div ui:field="mainPlaceholder" />
      </section>
      <section ui:field="footer" class="{bs.footer}">
        <div ui:field="footerPlaceholder" />
      </section>
    </section>
    <section class="{bs.info}">
      <div ui:field="infoPlaceholder" />
    </section>
  </gwt:HTMLPanel>
</ui:UiBinder>