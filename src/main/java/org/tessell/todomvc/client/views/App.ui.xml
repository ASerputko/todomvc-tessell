<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:gwt="urn:import:com.google.gwt.user.client.ui">
  <gwt:HTMLPanel ui:field="root">
    <section id="todoapp">
      <header id="header">
        <div ui:field="headerPlaceholder" />
      </header>
      <section id="main">
        <div ui:field="mainPlaceholder" />
      </section>
      <section id="footer">
        <div ui:field="footerPlaceholder" />
      </section>
    </section>
    <section id="info">
      <div ui:field="infoPlaceholder" />
    </section>
  </gwt:HTMLPanel>
</ui:UiBinder>